  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "concat": "^1.0.3",
    "node-sass": "^4.9.4",
    "postcss-cli": "^6.0.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
